  "transforms": [
    {
      "id": "prefix-keys",
      "description": "Adds a \"PREF\" prefix to all object keys.",
      "type": "jq",
      "ref": "https://tdn.github.io/bblocks-tutorial/_sources/exercise7/transforms/prefix-keys.jq",
      "code": "walk(if type == \"object\" then with_entries( .key |= \"PREF\\(.)\" ) else . end)",
      "inputs": {
        "mediaTypes": [
          {
            "id": "json",
            "defaultExtension": "json",
            "label": "JSON",
            "mimeType": "application/json",
            "extensions": [
              "json"
            ]
          }
        ]
      },
      "outputs": {
        "mediaTypes": [
          {
            "id": "json",
            "defaultExtension": "json",
            "label": "JSON",
            "mimeType": "application/json",
            "extensions": [
              "json"
            ]
          }
        ]
      }
    },
    {
      "id": "prefix-predicates",
      "description": "Changes the \"http://example.com/\" prefix of every predicate to a URN",
      "type": "sparql-update",
      "ref": "https://tdn.github.io/bblocks-tutorial/_sources/exercise7/transforms/prefix-predicates.sparql",
      "code": "DELETE {\n  ?s ?p ?o\n}\nINSERT {\n  ?s ?p2 ?o\n}\nWHERE {\n  ?s ?p ?o\n  FILTER(STRSTARTS(STR(?p), \"http://example.com/\"))\n  BIND(URI(REPLACE(STR(?p), \"^http://example.com/\", \"urn:example:ex#\")) as ?p2)\n}",
      "inputs": {
        "mediaTypes": [
          {
            "id": "ttl",
            "defaultExtension": "ttl",
            "label": "RDF/Turtle",
            "mimeType": "text/turtle",
            "aliases": [
              "turtle",
              "rdf/turtle"
            ],
            "extensions": [
              "ttl"
            ]
          },
          {
            "id": "rdf",
            "defaultExtension": "rdf",
            "label": "RDF/XML",
            "mimeType": "application/rdf+xml",
            "aliases": [
              "rdf/xml"
            ],
            "extensions": [
              "rdf"
            ]
          }
        ]
      },
      "outputs": {
        "mediaTypes": [
          {
            "id": "ttl",
            "defaultExtension": "ttl",
            "label": "RDF/Turtle",
            "mimeType": "text/turtle",
